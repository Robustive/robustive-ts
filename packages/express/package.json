{
  "name": "robustive-ts-express",
  "version": "0.2.0",
  "description": "Express add-on for robustive-ts.",
  "main": "./dist/robustive-express.umd.js",
  "module": "./dist/robustive-express.es.js",
  "types": "./types/index.d.ts",
  "files": [
    "/dist",
    "/types"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/robustive-express.es.js"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./dist/robustive-express.umd.js"
      }
    }
  },
  "scripts": {
    "build:clean": "shx rm -rf ./dist ./types",
    "build": "tsc && vite build",
    "prepare": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Robustive/Robustive-ts"
  },
  "author": "Yusuke SAITO <yusuke.saito@jibunstyle.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^5.2.1",
    "robustive-ts": "workspace:^"
  },
  "devDependencies": {
    "@types/express": "^5",
    "@types/node": "^20.11.26",
    "shx": "^0.3.4",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  }
}
