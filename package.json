{
  "name": "robustive-ts",
  "version": "0.2.0",
  "description": "Reactive and OBjective USecase Transactor for TypeScript.",
  "main": "./dist/robustive.umd.js",
  "module": "./dist/robustive.es.js",
  "types": "./types/index.d.ts",
  "files": [
    "/dist",
    "/type"
  ],
  "exports": {
    ".": {
      "import": "./dist/robustive.es.js",
      "require": "./dist/robustive.umd.js"
    }
  },
  "scripts": {
    "build:clean": "shx rm -rf ./dist ./types",
    "build": "tsc && vite build",
    "prepare": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Robustive/Robustive-ts"
  },
  "author": "Yusuke SAITO <yusuke.saito@jibunstyle.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "shx": "^0.3.4",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  }
}
